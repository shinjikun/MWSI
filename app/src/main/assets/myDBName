CREATE TABLE T_UPLOAD (
ULDOCNO              VARCHAR(20)          not null,
MRU                  VARCHAR(8),
ACCTNUM              VARCHAR(12),
BASIC_CHARGE         DECIMAL(14,2),
DISCOUNT             DECIMAL(14,2),
CERA                 DECIMAL(14,2),
FCDA                 DECIMAL(14,2),
ENV_CHARGE           DECIMAL(14,2),
SEWER_CHARGE         DECIMAL(14,2),
MSC_AMOUNT           DECIMAL(14,2),
SC_DISCOUNT          DECIMAL(14,2),
TOTCHRG_WO_TAX       DECIMAL(14,2),
VAT_CHARGE           DECIMAL(14,2),
TOT_CURR_CHARGE      DECIMAL(14,2),
PREVUNPAID           DECIMAL(14,2),
SEPTIC_CHARGE        DECIMAL(14,2),
CHANGESIZE_CHARGE    DECIMAL(14,2),
RESTORATION_CHARGE   DECIMAL(14,2),
MISC_CHARGE          DECIMAL(14,2),
INSTALL_SEWER_CHARGE DECIMAL(14,2),
ADVANCES             DECIMAL(14,2),
INSTALL_SEWER_DUE    DECIMAL(14,2),
INSTALL_AR_DUE       DECIMAL(14,2),
INSTALL_ADV_DUE      DECIMAL(14,2),
INSTALL_WATER_DUE    DECIMAL(14,2),
INSTALL_WATER_CHARGE DECIMAL(14,2),
REOPENING_FEE        DECIMAL(14,2),
METER_CHARGES        DECIMAL(14,2),
GD_CHARGE            DECIMAL(14,2),
OTHER_CHARGES        DECIMAL(14,2),
SUBTOTAL_AMT         DECIMAL(14,2),
TOTAL_AMT_DUE        DECIMAL(14,2),
PRINT_COUNT          INTEGER,
PRINT_TAG            INTEGER,
primary key (ULDOCNO),
foreign key (PRINT_TAG)
      references R_PRINT_TAG (PRINT_TAG),
foreign key (MRU)
      references T_MRU_INFO (MRU)
);
CREATE TABLE T_SAP_DETAILS (
RECNUM               NUMERIC(6)           not null,
SAP_LINE_CODE        VARCHAR(10),
SAPDOCNO             VARCHAR(20),
ACCTNUM              VARCHAR(12),
