BLOCKED_COUNT        INTEGER,
READ_METERS          INTEGER,
UNREAD_METERS        INTEGER,
UNDELIV_BILLS        INTEGER,
primary key (MRU),
foreign key (BC_CODE)
      references R_BUSINESS_CENTER (BC_CODE)
);
