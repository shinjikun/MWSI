OLD_PRICE            DECIMAL(6,2),
OLD_AMOUNT           DECIMAL(14,2),
TOTAL_AMOUNT         DECIMAL(14,2),
primary key (RECNUM),
foreign key (SAP_LINE_CODE)
      references R_SAP_LINEREF (SAP_LINE_CODE)
);
CREATE TABLE T_PROMO_MESSAGE (
PROMO_MSG_ID         INTEGER              not null,
PROMO_STARTDT        DATE,
PROMO_ENDDT          DATE,
